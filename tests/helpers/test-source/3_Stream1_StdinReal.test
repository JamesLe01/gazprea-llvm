procedure main() returns integer {
    {
        real a;
        integer i = 0;
        loop while (i < 10) {
            a <- std_input;
            a -> std_output;
            if (i != 9) ' ' -> std_output;
            i = i + 1;
        }
    }

    return 0;
}
