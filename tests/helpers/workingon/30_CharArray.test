typedef integer int;
typedef character char;

procedure main() returns int {
    char[*] hello = ['h', 'e', 'l', 'l', 'o'];
    string[5+5+4+] wow = hello || "world";

    int test = 100;

    string[test] identities = identity;
    string[test+test] nu = null;

    wow -> std_out; // Should print "helloworld"
    "\n" -> std_out; // Should make a new line

    identities -> std_out;
    nu -> std_out;

    wow = wow || [' ', 'y', 'e', 's'];

    return 0;
}
