typedef integer int;
typedef boolean bool;
typedef character char;
typedef real float;

const int INT_MAX = 2147483647;
//const int INT_MIN = -2147483648;
const float INT_MAX_IN_FLOAT = 2.14748e+09;
const float FLOAT_MAX = 3.40282347e+38;

procedure main() returns int {
    // Note, cannot type promote inside std_out

    false -> std_out; // F
    INT_MAX -> std_out; // 2147483647
    FLOAT_MAX -> std_out; // 3.40282e+38

    // Test all escape sequence
    '\0' -> std_out;
    '\a' -> std_out;
    '\b' -> std_out;
    '\t' -> std_out;
    '\n' -> std_out;
    '\r' -> std_out;
    '\"' -> std_out;
    '\'' -> std_out;
    '\\' -> std_out;

    var bool input = null;
    input <- std_input;

    var int integ = null;
    integ <- std_input;

    var char characters = null;
    characters <- std_input;

    var float flo = null;
    flo <- std_input;

    return 0;
}
