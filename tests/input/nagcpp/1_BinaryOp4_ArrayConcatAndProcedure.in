        call set415(str);

        strConcat -> std_output;
        '\n' -> std_output;
        str || " cat" -> std_output;
    }

    '\n' -> std_output;
    {
        var string[5] a = "hello";
        var string[*] b = " world";
        var string c = "hello";
        var string[6] d = " world";
        const temp = concatAndPrint(c, d);
        '\n' -> std_output;
        call concatAndPrint(a, b);
        '\n' -> std_output;
        temp -> std_output;
    }
    '\n' -> std_output;
