procedure set415(var str) {
    str[2..4] = "415";
}

procedure concatAndPrint(var string[5] str1, var string[*] str2) returns string {
    var string temp = str1 || str2;
    temp[1] = 'w';
    str1 || str2 -> std_output;
    '\n' -> std_output;
    temp -> std_output;
    return temp;
}

