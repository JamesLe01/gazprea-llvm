procedure changeTup4(var a) {
  a.b = 3;
}
procedure changeTup3(var a) {
  call changeTup4(a);
}
procedure changeTup2(var tuple(integer, integer b) a) {
  call changeTup4(a);
}
procedure changeTup1(var tuple(integer b, integer) a) {
  call changeTup4(a);
}

procedure main() returns integer {
  tuple(integer d, integer e) a = (1, 2);
  tuple(integer b, integer e) b = (1, 2);
  tuple(integer d, integer b) c = (1, 2);

  a.1 -> std_output;
  a.2 -> std_output;
  call changeTup1(a);
  a.1 -> std_output;
  a.2 -> std_output;
  call changeTup2(a);
  a.1 -> std_output;
  a.2 -> std_output;

  '\n' -> std_output;
  call changeTup3(b);
  b.1 -> std_output;
  b.2 -> std_output;
  call changeTup3(c);
  c.1 -> std_output;
  c.2 -> std_output;

  return 0;
}
